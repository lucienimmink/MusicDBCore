{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": false,
    "declaration": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "test"
  ]
}